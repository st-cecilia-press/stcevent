<ul>
    <% @people.each do |person| %>
      <li> 
            <%= link_to person.title, person_path(person) %> 
            (<%= link_to 'Edit', edit_person_path(person) %>)
            <%= button_to 'Delete', person, method: :delete, class: "btn btn-danger" %>
            </li>
    <% end %>
</ul>
