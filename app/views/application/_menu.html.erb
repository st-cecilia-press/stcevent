<ul class="nav navbar-nav">
  <% MenuItem.where(event: @event).order(:order).each do |menu_item| %>
    <li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" ><%= menu_item.name %><span class="caret"></span></a>

    <ul class="dropdown-menu">
      <% Page.where(menu_item: menu_item).order(:menu_order).each do |page| %>
        <li><%= link_to page.title, page.slug %></li>
      <% end %>
    </ul>
  <% end %>
</ul>
